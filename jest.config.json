{
  "name": "archetypes",
  "automock": false,
  "collectCoverage": false,
  "collectCoverageFrom": [
    "**/schemas/**"
  ],
  "coverageDirectory": "./coverage",
  "coveragePathIgnorePatterns": [
    "/generate-docs.js",
    "/lib/",
    "/node_modules/",
    "/__tests__/__fixtures__/",
    "/__tests__/__mocks__/",
    "/__tests__/"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  },
  "testPathIgnorePatterns": [
    "/lib/",
    "/__tests__/__fixtures__/",
    "/__tests__/__mocks__/"
  ],
  "verbose": true
}
